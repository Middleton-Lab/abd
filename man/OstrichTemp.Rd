\docType{data}
\name{OstrichTemp}
\alias{OstrichTemp}
\title{Ostrich Body and Brain Temperatures}
\format{A data frame with 6 observations on the following 3 variables.
\describe{ \item{ostrich}{a numeric vector identifying ostrich
number} \item{body.temp}{a numeric vector of body temperature in
\eqn{^{\circ}}{degrees }C} \item{brain.temp}{a numeric vector of
brain temperature in \eqn{^{\circ}}{degrees }C} }}
\source{
  Fuller, A., P.R. Kamerman, S.K. Maloney, G. Mitchell, and
  D. Mitchell. 2003. Variability in brain and arterial
  blood temperatures in free-ranging ostriches in their
  natural habitat. \emph{Journal of Experimental Biology}
  206: 1171-1181.
}
\description{
  Body and brain temperatures (\eqn{^{\circ}}{degrees }C)
  in free-ranging ostriches (\emph{Struthio camelus}) at
  the the Lichtenburg Game Breeding Centre, Lichtenburg,
  South Africa.
}
\examples{
data(OstrichTemp)
xyplot(brain.temp ~ body.temp, OstrichTemp)
}
\references{
  \url{http://jeb.biologists.org/cgi/content/abstract/206/7/1171}

  \url{http://www.sa-venues.com/game-reserves/nwp_lichtenburg.htm}
}
\keyword{datasets}

