\name{SocialSpiderColonies}
\alias{SocialSpiderColonies}
\docType{data}

\title{
Social Spiders
}

\description{
Web height above ground (cm) and colony size for 17 colonies of the spider \emph{Cryptophora citricola} in Gabon.
}

\usage{data(SocialSpiderColonies)}
\format{
  A data frame with 17 observations on the following 3 variables.
  \describe{
    \item{\code{colony}}{a numeric vector}
    \item{\code{height.cm}}{a numeric vector}
    \item{\code{number.spiders}}{a numeric vector}
  }
}

